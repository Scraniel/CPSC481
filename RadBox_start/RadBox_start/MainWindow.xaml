<Window x:Class="RadBox_start.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RadBox" Height="635" Width="1028" FontFamily="Kristen ITC"
        xmlns:local="clr-namespace:System.Windows.Controls;assembly=PresentationFramework">
    <Window.Background>
        <RadialGradientBrush>
            <GradientStop Color="LightBlue"/>
            <GradientStop Color="RoyalBlue" Offset="1.5"/>
        </RadialGradientBrush>
    </Window.Background>
    <Grid>
        <local:Frame Name="MainFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
